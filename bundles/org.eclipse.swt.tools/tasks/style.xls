<?xml version="1.0" encoding="ISO-8859-1"?>

<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:param name="TAG"/>
<xsl:param name="workdir"/>
<xsl:template match="/">
<project name="test" default="checkout_native_files" basedir=".">
<target name="checkout_native_files">
	<xsl:for-each select="classpath/classpathentry">
	<xsl:if test="@kind = 'src'">
			<cvs cvsRoot=":pserver:anonymous@dev.eclipse.org:/cvsroot/eclipse" reallyquiet="true">
			<xsl:attribute name="dest">
				<xsl:value-of select="$workdir"/>
			</xsl:attribute> 
			<commandline>
				<argument value="-Q"/>
				<argument value="co"/>
				<argument value="-r"/>
				<argument>
					<xsl:attribute name="value">
						<xsl:value-of select="$TAG"/>
					</xsl:attribute>
				</argument>
				<argument>
					<xsl:attribute name="value">org.eclipse.swt/<xsl:value-of select="@path"/>/library</xsl:attribute>
				</argument>
			</commandline>
			</cvs>
	</xsl:if>
	</xsl:for-each>
</target>
</project>
</xsl:template>
</xsl:stylesheet>
